{
  "Role": "arn:aws:iam::503122374618:role/GlueJobCalendlyRole",
  "Command": {
    "Name": "glueetl",
    "ScriptLocation": "s3://calendly-marketing-pipeline-data/jobs/silver_to_gold_glue.py",
    "PythonVersion": "3"
  },
  "DefaultArguments": {
    "--job-language": "python",
    "--enable-metrics": "true",
    "--enable-spark-ui": "true",
    "--enable-glue-datacatalog": "true",
    "--enable-continuous-cloudwatch-log": "true",
    "--TempDir": "s3://calendly-marketing-pipeline-data/glue-logs/",
    "--datalake-formats": "delta",
    "--additional-python-modules": "pydeequ==1.0.1",
    "--extra-jars": "s3://calendly-marketing-pipeline-data/jars/delta-spark_2.12-3.2.0.jar",
    "--conf": "spark.sql.extensions=io.delta.sql.DeltaSparkSessionExtension,spark.sql.catalog.spark_catalog=org.apache.spark.sql.delta.catalog.DeltaCatalog"
  },
  "GlueVersion": "4.0",
  "NumberOfWorkers": 2,
  "WorkerType": "G.1X"
}
